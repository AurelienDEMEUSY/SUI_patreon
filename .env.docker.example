# DePatreon — Docker Compose environment
# Copy to .env and customize: cp .env.docker.example .env

# Contract (from sui client publish)
NEXT_PUBLIC_PACKAGE_ID=0x778ecde37896bf33ce157208cbd90a3d7e42475de59875d66f9db34031258d12
NEXT_PUBLIC_PLATFORM_ID=0x21d04f36c70ce554b59913311a96a8287da4b146793b8640aaddac74fe7e5cce

# Network
NEXT_PUBLIC_SUI_NETWORK=testnet

